{
	"name": "api-docs-site",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"build": "next build",
		"dev": "next dev",
		"start": "next start",
		"preview": "next build && next start",
		"postinstall": "fumadocs-mdx",
		"tsc": "tsc --noEmit --pretty",
		"validate": "bun run check && bun run tsc",
		"check": "biome check .",
		"check:fix": "biome check --write .",
		"check:unsafe": "biome check --write --unsafe ."
	},
	"dependencies": {
		"@fumadocs/mdx-remote": "^0.2.3",
		"fumadocs-core": "14.4.0",
		"fumadocs-mdx": "11.1.1",
		"fumadocs-openapi": "^5.6.0",
		"fumadocs-ui": "14.4.0",
		"next": "15.0.3",
		"react": "^18.3.1",
		"react-dom": "^18.3.1"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@types/mdx": "^2.0.13",
		"@types/node": "22.9.0",
		"@types/react": "^18.3.12",
		"@types/react-dom": "^18.3.1",
		"autoprefixer": "^10.4.20",
		"postcss": "^8.4.48",
		"tailwindcss": "^3.4.14",
		"typescript": "^5.6.3"
	}
}